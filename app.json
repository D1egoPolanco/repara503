{
  "expo": {
    "name": "Repara503",
    "slug": "repara503",
    "version": "1.0.0",                        // ← REQUERIDO por EAS (versión de app)
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
   "icon": "./assets/images/repara503.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.diepoz.repara503",
      "versionCode": 1                        // ← REQUERIDO por EAS (incrementar con cada build)
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "399c717e-bd0f-4374-b124-508bfa4500e1"
      }
    }
  }
}
